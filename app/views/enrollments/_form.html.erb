<h2>Edit Enrollment Note</h2>

<%= form_with model: [@student, @enrollment], local: true do |form| %>
  <div class="field">
    <%= form.label :note %>
    <%= form.text_area :note %>
  </div>

  <div class="field">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>

  <div class="actions">
    <%= form.submit "Update" %> 
  </div>
<% end %>